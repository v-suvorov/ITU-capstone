<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="All Tests Suite">
    <parameter name="browserName" value="firefox" />
    <parameter name="envBaseURL" value="https://linkedin.com" />
    <parameter name="apiBaseURL" value="https://maps.googleapis.com/maps/api" />
    <parameter name="apiMethodBasePath" value="/distancematrix/"/>
    <parameter name="apiType" value="json" />
    <test name="Google Maps API Distance Matrix Test Suite">
        <classes>
            <class name="tests.API.MapsDistanceMatrixTestSuite" />
        </classes>
    </test>
    <test name="LinkedIn Experience Section Test Suite">
        <classes>
            <class name="tests.functional.ExperienceSectionTestSuite" />
        </classes>
    </test>
    <test name="LinkedIn Skill Section Test Suite">
        <classes>
            <class name="tests.functional.SkillsSectionTestSuite" />
        </classes>
    </test>
    <test name="LinkedIn Education Section Test Suite">
        <classes>
            <class name="tests.functional.EducationSectionTestSuite" />
        </classes>
    </test>

</suite>
